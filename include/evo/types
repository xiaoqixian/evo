#pragma once

namespace evo {

using size_t    = unsigned long;
using ptrdiff_t = size_t;
using usize     = size_t;
using u8        = unsigned char;
using u16       = unsigned short;
using u32       = unsigned int;
using u64       = unsigned long;
using i8        = char;
using i16       = short;
using i32       = int;
using i64       = long;

}
